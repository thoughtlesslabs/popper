--a silly game about popcorn

function _init()
	cls()
	kern()
end

function _update60()
	updatekern()
end

function kern()
		x={}
		y={}
		v={}
		for i=1,15 do 
			add(x,mid(1,rnd(110),110))
			add(y,mid(1,rnd(110),110))
			add(v,true)
		end
end

function drawkern()
	cls()	
	for i=1,#x do
		if x[i]+x[i]>20 then
			print("⬇️",x[i],y[i])
		else
			print("⬇️",x[i],y[i]+10)
		end
	end
end

function updatekern()

end

function _draw()
	drawkern()
end
