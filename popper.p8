__lua__
--popper
--thoughtless labs
--a silly game about popcorn

function _init()
	cls()
	mode="start"
end


function _update60()
	if mode=="start" then
		updatestart()
	elseif mode=="game" then
		updatekern()
	elseif mode=="retry" then
		updateretry()
	end
end

function updatestart()
	cls()
	mode="game"
	kern()
	pop=false
	if btnp(4) then
		updatekern()
	end
end

function kern()
		x={}
		y={}
		v={}
		for i=1,15 do 
			add(x,mid(1,rnd(110),110))
			add(y,mid(1,rnd(110),110))
			add(v,true)
		end
end

function updatekern()
	if btnp(4) and pop==false then
		for i=1,#x do
			v[i]=false
		end
 sfx(0)
 pop = true
	end
end

function drawkern()
	local i
	cls(5)	
	--rectfill(35,35,95,60,4)	
	for i=1,#x do
		if v[i] then
			spr(2,x[i],y[i])
		elseif pop then
			spr(1,x[i]+5,y[i]+5)
		end
	end
	
	if not(pop) then
		print("press üÖæÔ∏è",50,10,7)
	else
		retry()
	end
end	

function retry()
	rectfill(35,35,95,60,4)
	print("play again?",45,40,7)
	print("press ‚ùé",50,50,7)
	update_retry()
end

function update_retry()
	if btn(5) then
		mode="start"
	end
end

function _draw()
	if mode=="game" then
		drawkern()
	elseif mode=="start" then
		updatestart()
	elseif mode=="retry" then
		retry()
	end
end
